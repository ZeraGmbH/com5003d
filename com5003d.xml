<?xml version="1.0" encoding="ISO-8859-1"?>

<com5003dconfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">


	<connectivity>
		<debuglevel>0</debuglevel>
		<ethernet>
			<port>
				<server>6307</server>
				<resourcemanager>6312</resourcemanager>
			</port>
		</ethernet>
		<i2c>	
			<device>
				<node>/dev/i2c-3</node>
			</device>	
			<adress> 
				<master>32</master>
				<atmel>33</atmel>
				<flash>80</flash>
			</adress>
		</i2c>
		<fpga>
			<device> 
				<node>/dev/zFPGA1reg</node>
			</device>
		</fpga>		
	</connectivity>
	
	<resource>
		<sense>
			<m0>
				<value>UL1</value>
				<atmelchannel>1</atmelchannel>
				<dspchannel>0</dspchannel>
				<avail>true</avail>  
			</m0>
			<m1>
				<value>UL2</value>
				<atmelchannel>2</atmelchannel>
				<dspchannel>1</dspchannel>
				<avail>true</avail>  
			</m1>
			<m2>
				<value>UL3</value>
				<atmelchannel>3</atmelchannel>
				<dspchannel>2</dspchannel>
				<avail>true</avail>  
			</m2>
			<m3>
				<value>IL1</value>
				<atmelchannel>4</atmelchannel>
				<dspchannel>3</dspchannel>
				<avail>true</avail>  
			</m3>
			<m4>
				<value>IL2</value>
				<atmelchannel>5</atmelchannel>
				<dspchannel>4</dspchannel>
				<avail>true</avail>  
			</m4>
			<m5>
				<value>IL3</value>
				<atmelchannel>6</atmelchannel>
				<dspchannel>5</dspchannel>
				<avail>true</avail>  
			</m5>
			<m6>
				<value>REF1</value>
				<atmelchannel>1</atmelchannel>
				<dspchannel>0</dspchannel>
				<avail>true</avail>  
			</m6>
			<m7>
				<value>REF2</value>
				<atmelchannel>2</atmelchannel>
				<dspchannel>1</dspchannel>
				<avail>true</avail>  
			</m7>
			<m8>
				<value>REF3</value>
				<atmelchannel>3</atmelchannel>
				<dspchannel>2</dspchannel>
				<avail>true</avail>  
			</m8>
			<m9>
				<value>REF4</value>
				<atmelchannel>4</atmelchannel>
				<dspchannel>3</dspchannel>
				<avail>true</avail>  
			</m9>
			<m10>
				<value>REF5</value>
				<atmelchannel>5</atmelchannel>
				<dspchannel>4</dspchannel>
				<avail>true</avail>  
			</m10>
			<m11>
				<value>REF6</value>
				<atmelchannel>6</atmelchannel>
				<dspchannel>5</dspchannel>
				<avail>true</avail>  
			</m11>
		</sense>
			
		<sample>
			<s0>
				<name>S0</name>
				<avail>true</avail>
			</s0>
		</sample>
		
		<source>	
			<fpzout>
				<fo0>
					<name>FPZ1</name>
					<dspserver>6310</dspserver>
					<dspchannel>0</dspchannel>
					<avail>true</avail>
				</fo0>
				<fo1>
					<name>FPZ2</name>
					<dspserver>6310</dspserver>
					<dspchannel>1</dspchannel>
					<avail>true</avail>
				</fo1>
				<fo2>
					<name>FPZ3</name>
					<dspserver>6310</dspserver>
					<dspchannel>2</dspchannel>
					<avail>true</avail>
				</fo2>
				<fo3>
					<name>FPZ4</name>
					<dspserver>6310</dspserver> 
					<dspchannel>3</dspchannel>
					<avail>true</avail>
				</fo3>
			</fpzout>
