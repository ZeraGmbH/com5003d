<?xml version="1.0" encoding="ISO-8859-1"?>

<com5003dconfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">


	<connectivity>
		<debuglevel>0</debuglevel>
		<ethernet>
			<ipadress>
				<resourcemanager>192.168.6.124</resourcemanager>
			</ipadress>	
			<port>
				<server>6307</server>
				<resourcemanager>6312</resourcemanager>
			</port>
		</ethernet>
		<i2c>	
			<device>
				<node>/dev/i2c-3</node>
			</device>	
			<adress> 
				<master>32</master>
				<atmel>33</atmel>
				<flash>80</flash>
			</adress>
		</i2c>
		<fpga>
			<device> 
				<node>/dev/zFPGA1reg</node>
			</device>
		</fpga>		
	</connectivity>
	
	<resource>
		<sense>
			<m0>
				<alias1>UL1</alias1>
				<alias2>REF1</alias2>
				<ctrlchannel>1</ctrlchannel>
				<dspchannel>0</dspchannel>
				<avail>true</avail>  
			</m0>
			<m1>
				<alias1>UL2</alias1>
				<alias2>REF2</alias2>
				<ctrlchannel>2</ctrlchannel>
                                <dspchannel>2</dspchannel>
				<avail>true</avail>  
			</m1>
			<m2>
				<alias1>UL3</alias1>
				<alias2>REF3</alias2>
				<ctrlchannel>3</ctrlchannel>
                                <dspchannel>4</dspchannel>
				<avail>true</avail>  
			</m2>
			<m3>
				<alias1>IL1</alias1>
				<alias2>REF4</alias2>
				<ctrlchannel>4</ctrlchannel>
                                <dspchannel>1</dspchannel>
				<avail>true</avail>  
			</m3>
			<m4>
				<alias1>IL2</alias1>
				<alias2>REF5</alias2>
				<ctrlchannel>5</ctrlchannel>
                                <dspchannel>3</dspchannel>
				<avail>true</avail>  
			</m4>
			<m5>
				<alias1>IL3</alias1>
				<alias2>REF6</alias2>
				<ctrlchannel>6</ctrlchannel>
				<dspchannel>5</dspchannel>
				<avail>true</avail>  
			</m5>
		</sense>
			
		<sample>
			<s0>
				<alias>S0</alias>
				<avail>true</avail>
			</s0>
		</sample>
		
		<source>	
			<fpzout>
				<fo0>
					<alias>FPZ1</alias>
					<dspserver>6310</dspserver>
					<dspchannel>0</dspchannel>
					<avail>true</avail>
				</fo0>
				<fo1>
					<alias>FPZ2</alias>
					<dspserver>6310</dspserver>
					<dspchannel>1</dspchannel>
					<avail>true</avail>
				</fo1>
				<fo2>
					<alias>FPZ3</alias>
					<dspserver>6310</dspserver>
					<dspchannel>2</dspchannel>
					<avail>true</avail>
				</fo2>
				<fo3>
					<alias>FPZ4</alias>
					<dspserver>6310</dspserver> 
					<dspchannel>3</dspchannel>
					<avail>true</avail>
				</fo3>
			</fpzout>
		</source>
	</resource>
</com5003dconfig> 
