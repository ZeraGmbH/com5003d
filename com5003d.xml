				<resourcemanager>6312</resourcemanager>
			</port>
		</ethernet>
		<i2c>	
			<device>
				<node>/dev/i2c-3</node>
			</device>	
			<adress> 
				<master>32</master>
				<atmel>33</atmel>
				<flash>80</flash>
			</adress>
		</i2c>
		<fpga>
			<device> 
				<node>/dev/zFPGA1reg</node>
			</device>
